{
    "extends": "airbnb-base",
    "env": {
        "mocha": true
    },
    "rules": {
        "arrow-parens": "off",
        "strict": 0,
        "indent": [
            2,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "eol-last": [
            0
        ],
        "comma-dangle": 0
    }
}